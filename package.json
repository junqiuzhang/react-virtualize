{
  "name": "vue-virtualized",
  "version": "1.3.0",
  "description": "react virtualized render",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-virtualized.umd.js",
  "module": "./dist/vue-virtualized.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-virtualized.es.js",
      "require": "./dist/vue-virtualized.umd.js"
    }
  },
  "type": "module",
  "types": "./dist/vue-virtualized.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junqiuzhang/react-virtualize.git"
  },
  "author": "junqiuzhang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/junqiuzhang/react-virtualize/issues"
  },
  "homepage": "https://github.com/junqiuzhang/react-virtualize#readme",
  "keywords": [
    "react",
    "reactjs",
    "virtual",
    "virtualized",
    "list",
    "scrolling"
  ],
  "devDependencies": {
    "@types/react": "^17.0.10",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vite-plugin-react": "^4.0.1",
    "vue-tsc": "^0.1.7"
  },
  "dependencies": {
    "react": "^17.0.0"
  }
